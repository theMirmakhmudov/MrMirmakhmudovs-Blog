{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "root/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "your_secret_key",
    "DJANGO_SETTINGS_MODULE": "root.settings",
    "DATABASE_URL": ""
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "root/wsgi.py"
    }
  ]
}
